{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "/run.sh",
    "restartPolicyType": "ALWAYS",
    "healthcheckPath": ":8081",
    "healthcheckTimeout": 10,
    "healthcheckInterval": 5
  }
}
